@techreport{wu_new_1997,
	type = {Macroeconomics},
	title = {New Result in Theory of Consumption: Changes in Savings and Income Growth},
	lccn = {0000},
	shorttitle = {New Result in Theory of Consumption},
	url = {http://ideas.repec.org/p/wpa/wuwpma/9706007.html},
	abstract = {Hall (1978) theorized that future consumption could be written only as a function of its current consumption. Since this result is known to be wrong in data, we reexamine, from Flavin's original equations (1981), how they may have reached this conclusion. In the Appendix, we derive a generic permanent income equation and discuss Flavin's imprecision. Furthermore, by replacing one of Flavin's three basic equations and adding the definition of savings, we find that changes in savings should also be a function of labor income growth, a solution anticipated by Modigliani and Brumberg (1952, published 1979).},
	number = {9706007},
	urldate = {2013-03-11},
	institution = {{EconWPA}},
	author = {Wu, Cheng K.},
	year = {1997},
	keywords = {consumption growth saving income},
	file = {RePEc Snapshot:/Users/kenjimyzk/Library/Application Support/Firefox/Profiles/74pwi9mv.default/zotero/storage/PRUT2Q98/9706007.html:text/html;Wu_1997_New Result in Theory of Consumption.pdf:/Users/kenjimyzk/Library/Application Support/Firefox/Profiles/74pwi9mv.default/zotero/storage/TVQEUPRC/Wu_1997_New Result in Theory of Consumption.pdf:application/pdf}
},

@techreport{bakhshi_sensitivity_2000,
	type = {Bank of England working paper},
	title = {The sensitivity of aggregate consumption to human wealth},
	lccn = {0006},
	url = {http://ideas.repec.org/p/boe/boeewp/108.html},
	abstract = {The Permanent Income Hypothesis ({PIH)} assumes that individuals base their decisions on lifetime wealth, not current income. Textbook versions of the {PIH} predict that the elasticity of consumption with respect to human wealth is equal to the share of human wealth in total wealth. Comparing calibrated wealth shares with econometrically estimated elasticities amounts to a simple test of the {PIH.} In the United Kingdom, aggregate consumption is found to be more sensitive to changes in human wealth than is predicted by the {PIH.} This does not appear to be explained by a simple, but common, treatment of credit constraints.},
	number = {108},
	urldate = {2013-03-11},
	institution = {Bank of England},
	author = {Bakhshi, Hasan},
	year = {2000},
