@techreport{campbell_consumption_1990,
	type = {{NBER} Working Paper},
	title = {Consumption, Income, and Interest Rates: Reinterpreting the Time Series Evidence},
	lccn = {0003},
	shorttitle = {Consumption, Income, and Interest Rates},
	url = {http://ideas.repec.org/p/nbr/nberwo/2924.html},
	abstract = {This paper proposes that the time-series data on consumption, income, and interest rates are best viewed as generated not by a single representative consumer but by two groups of consumers. Half the consumers are forward-looking and consume their permanent income, but are extremely reluctant to substitute consumption temporarily. Half the consumers follow the "rule of thumb" of consuming their current income. The paper documents three empirical regularities that, it argues, are best explained by this medal. First, expected changes in income are associated with expected changes in consumption. Second, expected real interest rates are not associated with expected changes in consumption. Third, periods in which consumption is high relative to income are typically followed by high growth in income. The paper concludes by briefly discussing the implications of these findings for economic policy and economic research.},
	number = {2924},
	urldate = {2013-03-11},
	institution = {National Bureau of Economic Research, Inc},
	author = {Campbell, John Y. and Mankiw, N. Gregory},
	year = {1990},
	file = {Campbell_Mankiw_1990_Consumption, Income, and Interest Rates.pdf:/Users/kenjimyzk/Library/Application Support/Firefox/Profiles/74pwi9mv.default/zotero/storage/HDEX9TT4/Campbell_Mankiw_1990_Consumption, Income, and Interest Rates.pdf:application/pdf;RePEc Snapshot:/Users/kenjimyzk/Library/Application Support/Firefox/Profiles/74pwi9mv.default/zotero/storage/5KB8NGH2/2924.html:text/html}
},

@techreport{campbell_permanent_1991,
	type = {{NBER} Working Paper},
	title = {Permanent Income, Current Income, and Consumption},
	lccn = {0000},
	url = {http://ideas.repec.org/p/nbr/nberwo/2436.html},
	abstract = {This paper reexamines the consistency of the permanent income hypothesis with aggregate, post-war, United States data. The permanent income hypothesis is nested within a more general model in which a fraction of income accrues to individuals who consume their current income rather than their permanent income. This fraction is estimated to be 40 or 50 percent, indicating a substantial departure from the permanent income hypothesis. This finding is robust to various statistical problems that have plagued previous work, such as time aggregation, and cannot be easily explained by appealing to changes in the real interest rate or to non-separabilities in the utility function.},
	number = {2436},
	urldate = {2013-03-11},
	institution = {National Bureau of Economic Research, Inc},
	author = {Campbell, John Y. and Mankiw, N. Gregory},
	year = {1991},
